{
  "name": "ui",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --filter=ui-*; turbo run dev --filter=*-playground",
    "dev:vue-playground": "turbo run dev --filter=vue-playground",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "turbo run test",
    "test:ui-core": "turbo run test --filter=ui-core",
    "test:ui-nuxt": "turbo run test --filter=ui-nuxt",
    "prepare:ui-core": "turbo run prepare --filter=ui-core",
    "prepare:ui-nuxt": "turbo run prepare --filter=ui-nuxt",
    "prepare:ui-vue": "turbo run prepare --filter=ui-vue"
  },
  "devDependencies": {
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "packageManager": "yarn@1.22.19"
}
