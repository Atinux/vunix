{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".nuxt/**"],
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "dependsOn": ["^dev"],
      "cache": false
    },
    "test": {
      "cache": false
    },
    "prepare": {
      "cache": false
    },
    "deploy": {
      "dependsOn": ["test", "build"],
      "outputs": [
        "dist/**"
      ]
    }
  }
}
